<?xml version='1.0'?>
<sdf version='1.7'>
  <model name="mule_car">

    <static>false</static>

    <!--The model and wheels for our car-->
    <link name='chassis'>
            <pose>0 0 0.1 0 0 0</pose>
            <!-- <inertial>
              <mass>0.8</mass>
              <inertia> 
                <ixx>0.001</ixx>
                <ixy>0.0</ixy>
                <ixz>0.0</ixz>
                <iyy>0.001</iyy>
                <iyz>0.0</iyz>
                <izz>0.001</izz>
              </inertia>
            </inertial> -->
            <collision name='collision'>
              <pose>0 0 0.02 0 0 0</pose>
              <geometry>
                <box>
                  <size>.275 .1 .05</size>
                </box>
              </geometry>
            </collision>

            <visual name='visual'>
              <geometry>
                <mesh>
                    <uri>/home/jyu34/Documents/wolfwagen/wolfwagen/gazebo_sim/carmodel.dae</uri>
                    <scale>1 1 1</scale>
                    <pose>0 0 0.05 0 0 0</pose>
                </mesh>
              </geometry>
            </visual>
    </link>

    <link name="front_left_wheel">
        <pose>0.057 0.07 0.1 0 1.5707 1.5707</pose>
        <inertial>
          <mass>0.13</mass>
          <inertia> 
            <ixx>0.001</ixx>
            <ixy>0.0</ixy>
            <ixz>0.0</ixz>
            <iyy>0.001</iyy>
            <iyz>0.0</iyz>
            <izz>0.001</izz>
          </inertia>
        </inertial>      
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.035</radius>
              <length>.01</length>
            </cylinder>
          </geometry>
        </collision>

        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.035</radius>
              <length>.01</length>
            </cylinder>
          </geometry>
        </visual>
    </link>

    <link name="front_right_wheel">
        <pose>0.057 -0.07 0.1 0 1.5707 1.5707</pose>
        <inertial>
          <mass>0.13</mass>
          <inertia> 
            <ixx>0.001</ixx>
            <ixy>0.0</ixy>
            <ixz>0.0</ixz>
            <iyy>0.001</iyy>
            <iyz>0.0</iyz>
            <izz>0.001</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.035</radius>
              <length>.01</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.035</radius>
              <length>.01</length>
            </cylinder>
          </geometry>
        </visual>
    </link>

    <link name="back_left_wheel">
        <pose>-0.057 0.07 0.1 0 1.5707 1.5707</pose>
        <inertial>
          <mass>0.13</mass>
          <inertia> 
            <ixx>0.001</ixx>
            <ixy>0.0</ixy>
            <ixz>0.0</ixz>
            <iyy>0.001</iyy>
            <iyz>0.0</iyz>
            <izz>0.001</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.035</radius>
              <length>.01</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.035</radius>
              <length>.01</length>
            </cylinder>
          </geometry>
        </visual>
    </link>

    <link name="back_right_wheel">
        <pose>-0.057 -0.07 0.1 0 1.5707 1.5707</pose>
        <inertial>
          <mass>0.13</mass>
          <inertia> 
            <ixx>0.001</ixx>
            <ixy>0.0</ixy>
            <ixz>0.0</ixz>
            <iyy>0.001</iyy>
            <iyz>0.0</iyz>
            <izz>0.001</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.035</radius>
              <length>.01</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.035</radius>
              <length>.01</length>
            </cylinder>
          </geometry>
        </visual>
    </link>

    <joint type="revolute" name="front_left_wheel_hinge">
        <pose>0 0 -0.03 0 0 0</pose>
        <child>front_left_wheel</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 0 1</xyz>
        </axis>
    </joint>

    <joint type="revolute" name="front_right_wheel_hinge">
        <pose>0 0 0.03 0 0 0</pose>
        <child>front_right_wheel</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 0 1</xyz>
        </axis>
    </joint>

    <joint type="revolute" name="back_left_wheel_hinge">
        <pose>0 0 -0.03 0 0 0</pose>
        <child>back_left_wheel</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 0 -1</xyz>
        </axis>
    </joint>

    <joint type="revolute" name="back_right_wheel_hinge">
        <pose>0 0 0.03 0 0 0</pose>
        <child>back_right_wheel</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 0 -1</xyz>
        </axis>
    </joint>

    <!--The lidar sensor-->
    <link name="lidar">
      <pose>-0.07 0 0.15 0 0 0</pose>
      <!-- <inertial>
        <mass>0.22</mass>
        <inertia> 
          <ixx>0.001</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.001</iyy>
          <iyz>0.0</iyz>
          <izz>0.001</izz>
        </inertia>
      </inertial> -->
      <sensor name="rplidar" type="ray">
        <always_on>true</always_on>
        <update_rate>30</update_rate>
        <visualize>0</visualize>
        <topic>scan</topic>
        <ray>
          <scan>
            <horizontal>
              <samples>360</samples>
              <resolution>1</resolution>
              <min_angle>0</min_angle>
              <max_angle>6.28</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.1</min>
            <max>12</max>
            <resolution>0.2</resolution>
          </range>
        </ray>
        <plugin name="laser" filename="libgazebo_ros_ray_sensor.so">
          <ros>
            <!-- <namespace>/fake_car</namespace> -->
            <argument>~/out:=scan</argument>
          </ros>
          <output_type>sensor_msgs/LaserScan</output_type>
          <frame_name>chassis</frame_name>
        </plugin>
      </sensor>
      <visual name="visual">
        <geometry>
          <box>
            <size>.3 .3 .3</size>
          </box>
        </geometry>
      </visual>
    </link>

    <joint name="lidar_joint" type="fixed">
      <pose>-0.07 0 0.15 0 0 0</pose>
      <parent>chassis</parent>
      <child>lidar</child>
      <plugin name="lidar_joint_publisher" filename="libgazebo_ros_joint_state_publisher.so">
        <joint_name>lidar_joint</joint_name>
        <update_rate>500</update_rate>
      </plugin>
    </joint>


  <!--Camera Time -->
  <link name="camera">
    <pose>-0.1 0 0.14 0 0 3.14</pose>
    <!-- <inertial>
      <mass>0.05</mass>
      <inertia> 
        <ixx>0.001</ixx>
        <ixy>0.0</ixy>
        <ixz>0.0</ixz>
        <iyy>0.001</iyy>
        <iyz>0.0</iyz>
        <izz>0.001</izz>
      </inertia>
    </inertial> -->
    <sensor type="camera" name="pi_camera">
      <camera>
        <horizontal_fov>1.047</horizontal_fov>
        <image>
          <width>320</width>
          <height>240</height>
        </image>
        <clip>
          <near>0.1</near>
          <far>100</far>
        </clip>
      </camera>
      <always_on>1</always_on>
      <update_rate>5</update_rate>
      <visualize>true</visualize>
      <plugin name="camera" filename="libgazebo_ros_camera.so">
        <ros>
          <namespace>/fake_car</namespace>
          <argument>~/out:=images</argument>
        </ros>
        <output_type>sensor_msgs/Image</output_type>
      </plugin>
    </sensor>

    <visual name="visual">
      <geometry>
        <box>
          <size>0.01 0.03 0.025</size>
        </box>
      </geometry>
    </visual>

  </link>

  <joint name="camera_joint" type="fixed">
    <parent>chassis</parent>
    <child>camera</child>
  </joint>


  <plugin name='diff_drive' filename='libgazebo_ros_diff_drive.so'>

    <ros>
      <namespace>/fake_car</namespace>
      <!-- <argument>cmd_vel:=cmd_vel</argument> -->
    </ros>

    <!-- optional -->
    <update_rate>500</update_rate>

    <!-- Specifies how many wheel pairs exist -->
    <num_wheel_pairs>2</num_wheel_pairs>

    <!-- front wheels -->
    <left_joint>front_left_wheel_hinge</left_joint>
    <right_joint>front_right_wheel_hinge</right_joint>

    <!-- back wheels -->
    <left_joint>back_left_wheel_hinge</left_joint>
    <right_joint>back_right_wheel_hinge</right_joint>

    <!-- kinematics -->
    <wheel_separation>0.14</wheel_separation>
    <wheel_diameter>0.035</wheel_diameter>

    <wheel_separation>0.14</wheel_separation>
    <wheel_diameter>0.035</wheel_diameter>

    <!-- limits -->
    <max_wheel_torque>20</max_wheel_torque>
    <max_wheel_acceleration>1.0</max_wheel_acceleration> 

    <!-- output -->
    <publish_odom>true</publish_odom>
    <publish_odom_tf>true</publish_odom_tf>
    <publish_wheel_tf>true</publish_wheel_tf>

    <odometry_frame>odom</odometry_frame>
    <robot_base_frame>chassis</robot_base_frame>

  </plugin> 

  </model>
</sdf>
